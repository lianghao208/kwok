# Let's getting started with kwokctl!
kwokctl create cluster

# Get all clusters.
kwokctl get clusters

# Create a node.
kubectl apply -f ./test/kwok/fake-node.yaml

# Get all nodes.
kubectl get node

# Apply a deployment.
kubectl apply -f ./test/kwok/fake-deployment.yaml

# Get all pods.
kubectl get pod

# Delete the cluster.
kwokctl delete cluster

# That's all, enjoy it!
clear
